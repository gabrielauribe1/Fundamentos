void leernum (int *num, int *potencia)
{
  printf("Introduzca el n√∫mero que desea elevar: \n");
  scanf("%d", num);
  printf("Introduzca la potencia: \n");
  scanf("%d", potencia);
}
void elevar (int num, int potencia, int *resultado)
{
  int i;
  for(i=1, i<=potencia, i++);
  *resultado=num;
}
